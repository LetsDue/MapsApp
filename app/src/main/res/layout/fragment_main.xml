<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainFragment"
        android:id="@+id/parentLayout">


    <ImageView
            android:src="@android:drawable/stat_sys_phone_call"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:id="@+id/imageView" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/imageView3" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"/>
    <ImageView
            android:src="@android:drawable/sym_action_email"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:id="@+id/imageView3"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintBottom_toTopOf="@+id/divider6"/>
    <TextView
            android:text="@string/contactNumber"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:id="@+id/phoneText"
            app:layout_constraintStart_toEndOf="@+id/imageView" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/emailText"
            android:clickable="true" android:onClick="onClick" android:focusable="true"/>
    <TextView
            android:text="karool019@gmail.com"
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:id="@+id/emailText"
            app:layout_constraintStart_toEndOf="@+id/imageView3"
            android:layout_marginStart="8dp" app:layout_constraintBottom_toTopOf="@+id/divider6"/>
    <ImageView
            android:src="@drawable/ic_categoriesicon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/categoriesIcon" app:layout_constraintEnd_toStartOf="@+id/searchIcon"
            android:layout_marginEnd="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            android:onClick="onClick" android:clickable="true"/>
    <ImageView
            android:src="@drawable/ic_search"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/searchIcon"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="@+id/divider2" android:layout_marginStart="8dp"
            android:clickable="true" android:onClick="onClick" android:focusable="true"/>
    <View
            android:id="@+id/divider"
            android:layout_width="395dp"
            android:layout_height="1dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintTop_toBottomOf="@+id/searchIcon" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginTop="4dp"/>
    <View
            android:id="@+id/divider2"
            android:layout_width="1dp"
            android:layout_height="380dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"/>
    <View
            android:id="@+id/divider3"
            android:layout_width="395dp"
            android:layout_height="2dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintBottom_toTopOf="@+id/phoneText"
            android:layout_marginBottom="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintHorizontal_bias="0.0"/>
    <TextView
            android:text="@string/placeholder"
            android:layout_width="380dp"
            android:layout_height="51dp"
            android:id="@+id/textView" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintBottom_toTopOf="@+id/divider3" app:layout_constraintHorizontal_bias="0.533"
            android:layout_marginBottom="20dp"/>
    <ImageView
            android:src="@drawable/ic_infoicon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/imageView6" app:layout_constraintEnd_toStartOf="@+id/divider2"
            android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="4dp"
            app:layout_constraintTop_toBottomOf="@+id/divider" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.923"/>
    <View
            android:id="@+id/divider6"
            android:layout_width="395dp"
            android:layout_height="1dp"
            android:background="?android:attr/listDivider"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>